{
  "pai-code-version":"2.0",
  "pai-module-name":"pai-bot-js-module-template",
  "pai-module-desc":"PAI Bot JS Module Template",
  "pai-module-settings":[
    "module-name","test"
  ],
  "required-modules":[
  ],
  "pai-code-commands":[
      {"command-name":"version", "js-function":"version"},
      {"command-name":"release-notes", "js-function":"get_release_notes"},
    {"command-name":"install_required_modules", "js-function":"install_required_modules"},
      {"command-name":"add-sample-entity", "js-function":"add_sample_entity",
        "params" : [
          {"name":"first-name", "label":"first-name", "description":"first name", "required":true},
          {"name":"last-name", "label":"last-name", "description":"last name", "required":true}
        ]
      },
    {"command-name":"get-sample-entity", "js-function":"get_sample_entity",
      "params" : [
        {"name":"filter", "label":"filter", "description":"query filter", "required":false}
      ]
    }
    ],
  "pai-entities" : {
    "include" : [
      "./src/pai-module/data/entities/sample-entity.json"
    ],
    "define" : [{
      "name": "pai-sample-entity",
      "fields": [
        {"name": "test-name","type": "string", "required": true},
        {"name": "writer","type": "string", "required": true}
      ]
    }]
  }
  ,
  "pai-ddb" : {
    "db-name" : "pai-module-db",
    "data-security" : "plain-text"
  },
  "pai-bot-web-interface": {
    "static-web-folders": ["public", "packages"],
    "web-services":[
      {"service-name":"home","js-function":"get_home"}
    ]
  },
  "pai-web-server": {
    "config": {
      "http": {
        "port" : 80
      },
      "https":{
        "port" : 443,
        "ssl-files" : [],
        "force-https" :false
      },
      "base-folder" : "$bot-folder/web/"
    },
    "static-web-folders": ["public", "packages"],
    "web-services":[
      {"service-name":"_home","js-function":"get_home"}
    ],
    "web-pages": [
      {"page-name": "home","file" :"home-page.json"}
    ]
  }
}




